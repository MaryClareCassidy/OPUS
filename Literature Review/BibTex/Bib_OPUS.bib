@ARTICLE{Raible2008,
  author    = {Raible, Daniel Edward},
  title     = {High Intensity Laser Power Beaming for Wireless Power Transmission},
  journal = {ETD ARchive},
  volume    = {576},
  year     = {2008},
  annotate    = {In this paper, methods of improving of efficiency of laser power beaming are examined. An example of a solar cell that is able to handle the intensity of laser power beaming is the vertical multi-junction (VMJ) solar cell it is most efficient in the 900-1000nm wavelengths and can provide up to 20 percent conversion efficiency. The performance decreases with temperature so a heat sink is needed. Conversion efficiency begins dropping dramatically at temperatures over 60°C. The optimal wavelength for laser energy is 976nm. Attempting to match the PV cell geometry to the gaussian beam increased efficiency. }
}

@ARTICLE{Jin2019,
